{
  "name": "apex-eval-demo",
  "version": "1.0.0",
  "type": "module",
  "description": "Complex microservices demo for APEX evaluation",
  "scripts": {
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=.*\\.test\\.js$",
    "test:integration": "jest --testPathPattern=.*\\.integration\\.test\\.js$",
    "lint": "eslint services shared --ext .js",
    "typecheck": "tsc --noEmit",
    "start:auth": "node services/auth/index.js",
    "start:user": "node services/user/index.js",
    "start:order": "node services/order/index.js",
    "start:notification": "node services/notification/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "mongoose": "^8.0.3",
    "redis": "^4.6.12",
    "amqplib": "^0.10.3",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "supertest": "^6.3.3"
  }
}